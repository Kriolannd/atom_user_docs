{
  "name": "atom_user_docs",
  "version": "0.0.1",
  "homepage": "https://github.com/Kriolannd/atom_user_docs",
  "description": "user docs for the project",
  "license": "ASE",
  "type": "module",
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:clean-dev": "vuepress dev docs --clean-cache",
    "docs:dev": "vuepress dev docs --host 0.0.0.0",
    "docs:update-package": "npx vp-update",
    "docs:build-gh-pages": "gh-pages -d docs/.vuepress/dist",
    "docs:deploy": "npm run docs:build && npm run docs:build-gh-pages"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "^2.0.0-rc.7",
    "@vuepress/plugin-shiki": "^2.0.0-rc.34",
    "@vuepress/theme-default": "^2.0.0-rc.11",
    "gh-pages": "^6.1.1",
    "vue": "^3.4.0",
    "vuepress": "^2.0.0-rc.7"
  },
  "dependencies": {
    "vuepress-theme-hope": "^2.0.0-rc.48"
  }
}
